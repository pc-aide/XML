<!-- Virtual Registry -->
    <Key Path="\REGISTRY\USER\[{AppVCurrentUserSID}]\Software\Microsoft\Office\16.0\Registration">
        <Value Type="REG_DWORD" Name="AcceptAllEulas" Data="1"/>
    </Key>
